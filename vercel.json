{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "ignoreCommand": "bash ignored-build-step.sh",
  "rewrites": [
    {
      "source": "/robots.txt",
      "destination": "/api/robots.txt.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/taxi-scraper.js": {
      "maxDuration": 30
    },
    "api/gbp-scraper.js": {
      "maxDuration": 30
    }
  }
}
